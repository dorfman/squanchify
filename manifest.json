{
  "manifest_version": 2,

  "name": "Squanchify",
  "description": "This extension replaces certain words with the word 'squanch'",
  "version": "1.0",

  "permissions": [],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Squanchify"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "squanch.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
