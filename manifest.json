{
  "manifest_version": 2,
  "name": "Squanchify",
  "description": "Get your squanch on.",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "squanch.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
